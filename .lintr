linters: with_defaults(
    commas_linter = NULL, # 39
    NULL
  )
exclusions: c(
    "R/xml.r"
  )
lintr_code_to_create_lintr:
  library(magrittr)
  library(dplyr)
  lintr::lint_package() %>%
    as.data.frame %>%
    group_by(linter) %>%
    tally(sort = TRUE) %$%
    sprintf("linters: with_defaults(\n    %s\n    NULL\n  )\n",
            paste0(linter, " = NULL, # ", n, collapse="\n    ")) %>%
    cat()
